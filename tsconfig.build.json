{
  "extends": "./tsconfig.json",
  "exclude": [
    "node_modules",
    "test",
    "dist",
    "**/*spec.ts",
    "**/*test.ts",
    "scripts",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "test/**/*",
    "coverage",
    "jest.config.js",
    "clean-redis.js"
  ],
  "compilerOptions": {
    "removeComments": true,
    "sourceMap": false,
    "inlineSources": false,
    "declaration": false,
    "declarationMap": false
  }
}