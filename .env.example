# Application Settings
NODE_ENV=development
PORT=3000

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672

# Flash API Configuration
FLASH_API_URL=https://api.flashapp.me/graphql
FLASH_API_KEY=your_flash_api_key

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+14155238886

# Maple AI Configuration
MAPLE_AI_URL=https://api.maple-ai.io
MAPLE_AI_API_KEY=your_maple_ai_api_key

# Security Configuration
JWT_SECRET=your_jwt_secret
MFA_TIMEOUT_SECONDS=300
OTP_EXPIRES_IN_SECONDS=600
SESSION_EXPIRES_IN_SECONDS=86400

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info